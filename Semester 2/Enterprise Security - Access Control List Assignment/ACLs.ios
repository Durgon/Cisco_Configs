ip access-list SALES-IN
  permit tcp 192.168.N.0 0.0.0.63 any eq ftp
  deny tcp 192.168.N.0 0.0.0.63 gt 1023 192.168.n.128 0.0.0.63 eq 80
  deny tcp 192.168.N.0 0.0.0.63 gt 1023 192.168.n+100.128 0.0.0.63 eq 80
  permit icmp 192.168.N.0 0.0.0.63 192.168.N.128 0.0.0.63 echo-reply
  deny icmp 192.168.N.0 0.0.0.63 192.168.N.128 0.0.0.63 echo
  deny icmp 192.168.N.0 0.0.0.63 192.168.N+100.128 0.0.0.63 echo

ip access-list SALES-OUT
  permit icmp 192.168.N.128 0.0.0.63 192.168.n.0 0.0.0.63 echo

ip access-list FINANCE-IN
  permit tcp 192.168.N.64 0.0.0.63 any eq ftp
  permit tcp 192.168.n.64 0.0.0.63 gt 1023 host 192.168.n.194 eq 80
  deny tcp 192.168.n.64 0.0.0.63 gt 1023 host 192.168.n+100.194 eq 80
  permit icmp 192.168.N.64 0.0.0.63 192.168.N.128 0.0.0.63 echo-reply
  deny icmp 192.168.N.0 0.0.0.63 192.168.N.128 0.0.0.63 echo
  deny icmp 192.168.N.0 0.0.0.63 192.168.N+100.128 0.0.0.63 echo

ip access-list FINANCE-OUT
  permit icmp 192.168.N.128 0.0.0.63 192.168.n.64 0.0.0.63 echo

ip access-list IT-IN
  permit tcp 192.168.N.128 0.0.0.63 any eq ftp
  permit icmp 192.168.n.128 0.0.0.63 any echo
  permit icmp 192.168.n.128 0.0.0.63 192.168.n+100.128 0.0.0.63 echo-reply
  permit udp 192.168.n.128 0.0.0.63 host 192.168.n.129 eq 69

  permit tcp 192.168.n.128 0.0.0.63 gt 1023 any eq 80


ip access-list IT-OUT
  permit icmp any 192.168.n.128 0.0.0.63 echo-reply
  permit icmp 192.168.n+100.128 0.0.0.63 192.168.n.128 0.0.0.63 echo

ip access-list WEB-IN
  permit tcp 192.168.N.192 0.0.0.63 any eq ftp
  permit icmp 192.168.N.192 0.0.0.63 192.168.N.128 0.0.0.63 echo-reply
  deny icmp 192.168.N.0 0.0.0.63 192.168.N.128 0.0.0.63 echo
  deny icmp 192.168.N.0 0.0.0.63 192.168.N+100.128 0.0.0.63 echo

ip access-list WEB-OUT
  permit icmp 192.168.N.128 0.0.0.63 192.168.n.192 0.0.0.63 echo
  permit tcp 192.168.n.0 0.0.0.193 host 192.168.n.194 eq ftp
  permit tcp 192.168.n.0 0.0.0.255 gt 1023 host 192.168.n.194 eq 80
  permit tcp 192.168.n+100.0 0.0.0.255 gt 1023 host 192.168.n.194 eq 80

access-list 1 permit 192.168.n.128 0.0.0.63
access-list 1 permit 192.168.n+100.128 0.0.0.63
!apply on vty lines
